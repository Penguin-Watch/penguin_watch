
    model {
    
    for (i in 1:N)
    {
      #both chicks alive at time step 1
      z[i,1] <- 1

      for (t in 2:L)
      { 

        #observation model
        y[i,t] ~ dbern(psight[i,t])
        psight[i,t] <- detect_p * z[i,t]

        #state model
        z[i,t] ~ dbern(palive[i,t])
        palive[i,t] <- surv_p * z[i,t-1]

      }
    }


    #priors
    surv_p ~ dunif(0,1)

    detect_p ~ dunif(0,1)
    
    }
